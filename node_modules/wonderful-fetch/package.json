{
  "name": "wonderful-fetch",
  "version": "1.1.12",
  "description": "A wrapper around fetch.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "npm run prepare && ./node_modules/mocha/bin/mocha test/ --recursive --timeout=30000",
    "prepare": "node -e \"require('prepare-package')()\"",
    "prepare:watch": "nodemon -w ./src -e '*' --exec 'npm run prepare'"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itw-creative-works/wonderful-fetch"
  },
  "keywords": [
    "rest api"
  ],
  "author": "ITW Creative Works",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/itw-creative-works/wonderful-fetch/issues"
  },
  "homepage": "https://itwcreativeworks.com",
  "runkitExampleFilename": "examples/process.js",
  "preparePackage": {
    "input": "./src",
    "output": "./dist",
    "replace": {}
  },
  "dependencies": {
    "fs-jetpack": "^4.3.1",
    "itwcw-package-analytics": "^1.0.4",
    "json5": "^2.2.1",
    "mime-types": "^2.1.35",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "mocha": "^8.4.0",
    "prepare-package": "^1.1.12"
  }
}